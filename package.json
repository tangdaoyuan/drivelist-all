{
  "name": "@suger-tdy/drivelist-all",
  "version": "0.1.3",
  "packageManager": "pnpm@7.5.0",
  "description": "Get driver list",
  "keywords": [
    "drivelist-all"
  ],
  "license": "MIT",
  "author": "tangdaoyuan <1197633750@qq.com> (https://github.com/tangdaoyuan/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangdaoyuan/drivelist-all.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts",
    "bin"
  ],
  "bin": {
    "drivelist": "./bin/drivelist.js"
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsx ./src/index.ts",
    "play": "npm -C playground run dev",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --tag --commit --push && pnpm publish"
  },
  "bugs": {
    "url": "https://github.com/tangdaoyuan/drivelist-all/issues"
  },
  "homepage": "https://github.com/tangdaoyuan/drivelist-all#readme",
  "dependencies": {
    "@suger-tdy/drivelist-linux": "^0.0.5",
    "@suger-tdy/drivelist-osx": "^0.0.11",
    "@suger-tdy/drivelist-windows": "^0.0.3",
    "picocolors": "^1.0.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@suger-tdy/eslint-config": "1.1.8",
    "@types/yargs": "^17.0.10",
    "bumpp": "8.2.1",
    "esbuild": "0.14.49",
    "eslint": "8.20.0",
    "tsx": "3.8.0",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "vite": "3.0.1",
    "vitest": "0.18.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
